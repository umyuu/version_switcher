{
    "manifest_version": 2,
	"author": "うみゅ",
	"description": "__MSG_G_description__",
    "name": "Document Version Switcher",
    "short_name": "__MSG_G_short_name__",
    "default_locale": "en",
	"content_scripts": [{
		"matches": [
            "http://127.0.0.1/javase/*",
            "https://docs.oracle.com/javase/*"
        ],
		"js": [
            "js/version_switcher.js"
        ],
        "css": ["resources/style.css"],
		"run_at": "document_start"
	}],
	"homepage_url": "https://github.com/umyuu/version_switcher",
    "web_accessible_resources":[
        "resources/template.html"
    ],
	"version": "1.0.2"
}