{
    "manifest_version": 2,
	"author": "うみゅ",
	"description": "__MSG_G_description__",
    "name": "Document Version Switcher",
    "short_name": "__MSG_G_short_name__",
    "default_locale": "en",
	"content_scripts": [{
		"matches": [ "https://docs.oracle.com/javase/*"],
